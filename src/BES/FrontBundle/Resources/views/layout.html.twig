                 <nav id="footer-nav" class="no-padding col-lg-6 col-lg-offset-3">
                    <ul class="list-unstyled text-center">
                    {% for element in skcmsVars.menus.main.children%}
                        
                        <li class="col-lg-3 col-md-3 col-sm-3 col-xs-3 no-padding">
                            <a href="{{element.link}}">{{element.name}}</a>
