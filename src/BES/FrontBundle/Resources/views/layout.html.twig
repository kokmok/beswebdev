        <meta name="msapplication-TileImage" content="/ms-icon-144x144.png">
        <meta name="theme-color" content="#ffffff">
        {% block css %}
        <link rel="stylesheet" href="{{asset('bundles/besfront/css/bootstrap.min.css')}}" />
        <link rel="stylesheet" href="{{asset('bundles/besfront/css/fonts.css')}}" />
        <link href='//fonts.googleapis.com/css?family=Raleway:400,300,700' rel='stylesheet' type='text/css'>
        <link rel="stylesheet" href="{{asset('bundles/besfront/css/main.css')}}" />
        {%endblock%}
    </head>
    <body>
        {% block header %}
        <header>
            <img src="{{asset('bundles/besfront/images/design/macbook.png')}}" alt="header mac book" id="header-image" />
            <a href="{{path('skcms_front_home')}}" id="logoLink"><img src="{{asset('bundles/besfront/images/design/logo.png')}}" alt="" id="logo" /></a>
            <div class="container">
                {%include 'SKCMSFrontBundle:menu:menu.html.twig' with {menu:skcmsVars.menus.main}%}
                 {#<nav id="header-nav" class="no-padding col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2 col-xs-12 col-xs-offset-0">
                        <ul class="list-unstyled text-center">
                            <li class="col-lg-3 col-md-3 col-sm-3 col-xs-3 no-padding">
                                <a href="#">Cours</a>
                                
                            </li>
                            <li class="col-lg-3 col-md-3 col-sm-3 col-xs-3 no-padding">
                                <a href="#">Eleves</a>
                                
                            </li>
                            <li class="col-lg-3 col-md-3 col-sm-3 col-xs-3 no-padding">
                                <a href="#">Contact</a>
                                
                            </li>
                            <li class="col-lg-3 col-md-3 col-sm-3 col-xs-3 no-padding">
                                <a href="#">Blog</a>
                            </li>
                            
                        </ul>
                        <div class="clearfix"></div>
                    </nav>#}
                    <div class="clearfix"></div>
            </div>
