            <div id="footer-icon" class="text-center"><img src="{{asset('bundles/besfront/images/design/footer-icon.png') }}" alt="" /></div>
            <div class="container">
                
                 <nav id="footer-nav" class="no-padding col-lg-6 col-lg-offset-3">
                    <ul class="list-unstyled text-center">
                    {% for element in skcmsVars.menus.main.children%}
                        
                        <li class="col-lg-3 col-md-3 col-sm-3 col-xs-3 no-padding">
                            <a href="{{element.link}}">{{element.name}}</a>
                        </li>
                    {%endfor%}
                            
                    </ul>
                    <div class="clearfix"></div>
                </nav>
                <div class="clearfix"></div>
            </div>
            <div class="" id="footer-links">
                <div id="credits" class="text-center col-md-3 col-lg-3 col-xs-12 col-sm-3 col-md-offset-2 col-lg-offset-2  col-sm-offset-2">
                    Design par <a href="http://www.whitecube.be/">Florian Hamoline</a> <br />section <a href="http://seraingsup.be/infographie.html">infographie</a>
                </div>
                
                <div id="" class="text-center col-md-2 col-lg-2 col-xs-12 col-sm-2">
                    <a href="{{path('skcms_front_home')}}"><img src="{{asset('bundles/besfront/images/design/logo.png')}}" alt="" id="footer-logo" /></a>
                </div>
                <div class="text-center col-md-3 col-lg-3 col-xs-12 col-sm-3" id="quick-links">
                    <a href="{{path('skcms_front_page',{slug:'contact'})}}"><img src="{{asset('bundles/besfront/images/design/contact-icon.png')}}" alt="" id="" /></a>
                </div>
                <div class="clearfix"></div>
            </div>
        </footer>
        
        
        
        <script src="//code.jquery.com/jquery-1.11.2.min.js" type="text/javascript"></script>
        {#<script src="{{ asset('bundles/besfront/js/masonry.pkgd.min.js') }}" type="text/javascript"></script>#}
        <script src="{{ asset('bundles/besfront/js/isotope.pkgd.min.js') }}" type="text/javascript"></script>

        <script src="{{asset('bundles/besfront/js/custom.js')}}" type="text/javascript"></script>
